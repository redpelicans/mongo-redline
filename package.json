{
  "name": "mongobless",
  "version": "2.1.0",
  "description": "es6 node.js thin mongodb document models ODM",
  "author": "Eric Basley <eric.basley@redpelicans.com>",
  "keywords": [
    "mongodb",
    "model",
    "document",
    "babel",
    "es6",
    "ODM"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/redpelicans/mongobless.git"
  },
  "dependencies": {
    "debug": "^2.2.0",
    "lodash": "^3.8.0",
    "mongodb": "^2.2.24",
    "nyc": "^10.1.2"
  },
  "contributors": [
    {
      "name": "Eric Basley",
      "email": "ebasley@gmail.com"
    }
  ],
  "devDependencies": {
    "async": "^0.9.0",
    "babel-cli": "^6.23.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "mocha": "^2.2.5",
    "should": "^6.0.1"
  },
  "scripts": {
    "build": "NODE_ENV=production babel -o index.js ./src",
    "coverage": "NODE_ENV=testing nyc yarn test",
    "test": "mocha --require babel-core/register --reporter spec test"
  },
  "license": "Apache",
  "nyc": {
    "exclude": [
      "test"
    ]
  },
  "babel": {
    "presets": [
      "latest",
      "stage-0"
    ],
    "plugins": [
      "transform-decorators-legacy"
    ]
  }
}
